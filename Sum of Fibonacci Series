#include <stdio.h>
int main() {
    int n, i,next;
    int a = 0, b = 1, sum=0;
    printf("Enter the number of terms: ");
    scanf("%d", &n);
    for (i = 0; i < n; i++) {
        sum += a;
        next = a + b;
        a = b;
        b = next;
    }
    printf("Sum of Fibonacci Series: %d",sum);
    return 0;
}
