#include <stdio.h>
#include <stdbool.h>
#define MAX 100
typedef struct Stack {
    int arr[MAX];
    int top;
} Stack;
void initStack(Stack* s) {
    s->top = -1;
}
bool isFull(Stack* s) {
    return s->top == MAX - 1;
}
bool isEmpty(Stack* s) {
    return s->top == -1;
}
bool push(Stack* s, int value) {
    if (isFull(s)) {
        return false; 
    }
    s->arr[++s->top] = value;
    return true;
}
int pop(Stack* s) {
    if (isEmpty(s)) {
        return -1; 
    }
    return s->arr[s->top--];
}
bool isValid(Stack* s) {
    return s->top >= -1 && s->top < MAX;
}
int main() {
    Stack s;
    initStack(&s);
    push(&s, 10);
    push(&s, 20);
    printf("Is stack valid? %s\n", isValid(&s) ? "Yes" : "No");
    return 0;
}
